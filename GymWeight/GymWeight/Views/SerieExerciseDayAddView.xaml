<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="GymWeight.Views.SerieExerciseDayAddView"
             xmlns:converters="clr-namespace:GymWeight.Converters;assembly=GymWeight">
    <ContentPage.Resources>
        <ResourceDictionary>
            <converters:DifficultyConverter x:Key="DifficultyConverter"></converters:DifficultyConverter>
        </ResourceDictionary>
    </ContentPage.Resources>
    <StackLayout>
        <StackLayout Orientation="Vertical">
            <Picker x:Name="ExerciseDayPicker" BindingContext="{Binding DifficultyList}"></Picker>
        </StackLayout>
    </StackLayout>
</ContentPage>